<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:OptiLearn.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pages="clr-namespace:Material.Demo.Pages"
        xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
        xmlns:controls="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OptiLearn.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="OptiLearn"
        Background="Transparent" FontFamily="Raleway Thin, Segoe UI, Roboto"
        TransparencyLevelHint="AcrylicBlur" ExtendClientAreaToDecorationsHint="False">

	<Grid Background="#701D1D1D" Margin="0 0 0 0" IsHitTestVisible="False">
	    <TabControl TabStripPlacement="Left" ZIndex="-1">
		    <TabItem Header="Home">
			    <Grid Background="#701D1D1D">
				    
			    </Grid>
		    </TabItem>
	    </TabControl>
	</Grid>
</Window>
